{
  "name": "home-accounting-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start-prod": "concurrently \"npm run prod\" \"npm run frontend\"",
    "frontend": "ng serve",
    "backend": "json-server --watch db.json --port 3000",
    "prod": "json-server --watch db-second.json --port 3000",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "vercel-start": "node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^18.2.0",
